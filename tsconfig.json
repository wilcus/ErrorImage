{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "build/dist",
    "allowJs": true,
    "checkJs": true,
    "jsx": "react",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "build",
    "rn-cli.config.js"
  ]
}
